    <!--skills-->
        
    <div class="col-12 vertical_range-4"></div>
    <div class="col-12 vertical_range-4 bg_color_aliceblue"></div>
    
    
    {{!-- Projects --}}
    <div class="col-12 d-flex justify-content-around bg_color_aliceblue" id="Projects">

        <div class="col-9 d-flex justify-content-start">
        

            {{!-- <hr style="color: white;height: 0.2rem; opacity:1;"> --}}
            
        {{#each projects}}
            {{!-- <hr style="color: white;height: 0.2rem; opacity:1;"> --}}
            <div class="card project_card" style="background-image: url('{{this.Image}}');">
                
                <div class="d-flex justify-content-around">
                    <div class="card-body project_card_body">
                        <h4 class="project_card_title">{{this.Name}}</h4>

                            <a class="project_card_link_page" title="" href="{{this.Link}}" 
                                target="_blank"> VIEW FULL PAGE..
                            </a>
                            
                            <a class="project_card_link_github" href="{{project.Github}}"
                                target="_blank"> GITHUB 
                            </a>
                        <div>
                            <div class="col-12 vertical_range-1"></div>
                            <h5>{{{this.Status}}}</h5>
                            
                            <p>{{{this.Decription}}}</p>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
            <div class="card project_card" style="background-image: url('bg2.png');">
                <h3 class="text_color_white">&nbsp updating</h3>
            </div>

        </div>
        <div class="col-3 text-secondary project_title">
            <h1 class=""> Projects , Course,..</h1>
        </div>
    </div>
    <!-- project -->
     
</div>
    
